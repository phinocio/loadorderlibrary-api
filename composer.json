{
    "name": "phinocio/loadorderlibrary-api",
    "type": "project",
    "description": "Load Order Library API",
    "keywords": ["skyrim", "Load Order Library", "Bethesda"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "cviebrock/eloquent-sluggable": "^11.0",
        "laravel/fortify": "^1.24.2",
        "laravel/framework": "^11.28.0",
        "laravel/sanctum": "^4.0.3",
        "laravel/tinker": "^2.10",
        "league/flysystem-aws-s3-v3": "^3.29",
        "sebastian/diff": "^6.0.2",
        "spatie/laravel-backup": "^9.1.2",
        "spatie/laravel-json-api-paginate": "^1.15.1",
        "spatie/laravel-query-builder": "^6.2.1",
        "wnx/laravel-backup-restore": "^1.4.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.1",
        "fakerphp/faker": "^1.23.1",
        "larastan/larastan": "^2.9.8",
        "laravel/pint": "^1.18.1",
        "laravel/sail": "^1.36.0",
        "laravel/telescope": "^5.2.3",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.5",
        "phpunit/phpunit": "^11.4.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": ["laravel/telescope"]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
